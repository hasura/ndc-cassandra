## 
##       4.0.0+
##       Section for management of default dependency information for use in a group of
##         POMs.
##     
DependencyManagement &=
  
  ## 
  ##           4.0.0+
  ##           The dependencies specified here are not used until they are referenced in a
  ##             POM within the group. This allows the specification of a "standard" version for a
  ##             particular dependency.
  ##         
  element dependencies {
    element dependency { Dependency }*
  }

## 
##       3.0.0+
##       
##         
##         The <code>&lt;dependency&gt;</code> element contains information about a dependency
##         of the project.
##         
##       
##     
Dependency &=
  element groupId {
    xsd:string
    >> a:documentation [
         "\x{a}" ~
         "          3.0.0+\x{a}" ~
         "          \x{a}" ~
         "            \x{a}" ~
         "            The project group that produced the dependency, e.g.\x{a}" ~
         "            <code>org.apache.maven</code>.\x{a}" ~
         "            \x{a}" ~
         "          \x{a}" ~
         "        "
       ]
  }
  & element artifactId {
      xsd:string
      >> a:documentation [
           "\x{a}" ~
           "          3.0.0+\x{a}" ~
           "          \x{a}" ~
           "            \x{a}" ~
           "            The unique id for an artifact produced by the project group, e.g.\x{a}" ~
           "            <code>maven-artifact</code>.\x{a}" ~
           "            \x{a}" ~
           "          \x{a}" ~
           "        "
         ]
    }
  & element version {
      xsd:string
      >> a:documentation [
           "\x{a}" ~
           "          3.0.0+\x{a}" ~
           "          \x{a}" ~
           "            \x{a}" ~
           "            The version of the dependency, e.g. <code>3.2.1</code>. In Maven 2, this can also be\x{a}" ~
           "            specified as a range of versions.\x{a}" ~
           "            \x{a}" ~
           "          \x{a}" ~
           "        "
         ]
    }
  & 
    ## 
    ##             default value is : jar
    (element type {
       xsd:string
       >> a:documentation [
            "\x{a}" ~
            "          4.0.0+\x{a}" ~
            "          \x{a}" ~
            "            \x{a}" ~
            "            The type of dependency, that will be mapped to a file extension, an optional classifier, and a few other attributes.\x{a}" ~
            "            Some examples are <code>jar</code>, <code>war</code>, <code>ejb-client</code>\x{a}" ~
            '            and <code>test-jar</code>: see <a href="../maven-core/artifact-handlers.html">default\x{a}' ~
            "            artifact handlers</a> for a list. New types can be defined by extensions, so this is not a complete list.\x{a}" ~
            "            \x{a}" ~
            "          \x{a}" ~
            "        "
          ]
     })
  & element classifier {
      xsd:string
      >> a:documentation [
           "\x{a}" ~
           "          4.0.0+\x{a}" ~
           "          \x{a}" ~
           "            \x{a}" ~
           "            The classifier of the dependency. It is appended to\x{a}" ~
           "            the filename after the version. This allows:\x{a}" ~
           "            <ul>\x{a}" ~
           "            <li>referring to attached artifact, for example <code>sources</code> and <code>javadoc</code>:\x{a}" ~
           '            see <a href="../maven-core/artifact-handlers.html">default artifact handlers</a> for a list,</li>\x{a}' ~
           "            <li>distinguishing two artifacts\x{a}" ~
           "            that belong to the same POM but were built differently.\x{a}" ~
           "            For example, <code>jdk14</code> and <code>jdk15</code>.</li>\x{a}" ~
           "            </ul>\x{a}" ~
           "            \x{a}" ~
           "          \x{a}" ~
           "        "
         ]
    }
  & element scope {
      xsd:string
      >> a:documentation [
           "\x{a}" ~
           "          4.0.0+\x{a}" ~
           "          \x{a}" ~
           "            \x{a}" ~
           "            The scope of the dependency - <code>compile</code>, <code>runtime</code>,\x{a}" ~
           "            <code>test</code>, <code>system</code>, and <code>provided</code>. Used to\x{a}" ~
           "            calculate the various classpaths used for compilation, testing, and so on.\x{a}" ~
           "            It also assists in determining which artifacts to include in a distribution of\x{a}" ~
           "            this project. For more information, see\x{a}" ~
           '            <a href="https://maven.apache.org/guides/introduction/introduction-to-dependency-mechanism.html">the\x{a}' ~
           "            dependency mechanism</a>. The default scope is <code>compile</code>.\x{a}" ~
           "            \x{a}" ~
           "          \x{a}" ~
           "        "
         ]
    }
  & element systemPath {
      xsd:string
      >> a:documentation [
           "\x{a}" ~
           "          4.0.0+\x{a}" ~
           "          \x{a}" ~
           "            \x{a}" ~
           "            FOR SYSTEM SCOPE ONLY. Note that use of this property is <b>discouraged</b>\x{a}" ~
           "            and may be replaced in later versions. This specifies the path on the filesystem\x{a}" ~
           "            for this dependency.\x{a}" ~
           "            Requires an absolute path for the value, not relative.\x{a}" ~
           "            Use a property that gives the machine specific absolute path,\x{a}" ~
           "            e.g. <code>${java.home}</code>.\x{a}" ~
           "            \x{a}" ~
           "          \x{a}" ~
           "        "
         ]
    }
  & 
    ## 
    ##           4.0.0+
    ##           Lists a set of artifacts that should be excluded from this dependency's
    ##             artifact list when it comes to calculating transitive dependencies.
    ##         
    element exclusions {
      element exclusion { Exclusion }*
    }
  & element optional {
      xsd:string
      >> a:documentation [
           "\x{a}" ~
           "          4.0.0+\x{a}" ~
           "          \x{a}" ~
           "            \x{a}" ~
           "            Indicates the dependency is optional for use of this library. While the\x{a}" ~
           "            version of the dependency will be taken into account for dependency calculation if the\x{a}" ~
           "            library is used elsewhere, it will not be passed on transitively. Note: While the type\x{a}" ~
           "            of this field is <code>String</code> for technical reasons, the semantic type is actually\x{a}" ~
           "            <code>Boolean</code>. Default value is <code>false</code>.\x{a}" ~
           "            \x{a}" ~
           "          \x{a}" ~
           "        "
         ]
    }

