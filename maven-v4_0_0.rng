        </a:documentation>
        
          
            <rng:zeroOrMore><rng:element name="resource"><rng:ref name="Resource"/></rng:element></rng:zeroOrMore>
          
        
      </rng:element></rng:optional></rng:optional><rng:optional><rng:optional><rng:element name="testResources">
