        </a:documentation>
        
          
            <rng:zeroOrMore><rng:element name="testResource"><rng:ref name="Resource"/></rng:element></rng:zeroOrMore>
          
        
      </rng:element></rng:optional></rng:optional><rng:optional><rng:optional><rng:element name="directory"><rng:data type="string"/><a:documentation>
